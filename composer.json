{
    "description": "NeosCMS package that is able to crawl the whole page and check the links for broken links",
    "type": "neos-package",
    "name": "unikka/neos-linkchecker",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.1",
        "neos/flow": "*",
        "neos/neos": "^4.0 || ^5.0 || ^7.0 || dev-master",
        "neos/swiftmailer": "^6.0 || ^7.0 || dev-master",
        "spatie/crawler": "^4.1",
        "league/csv": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Unikka\\LinkChecker\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Unikka.LinkChecker"
        },
        "applied-flow-migrations": [
            "Neos.Twitter.Bootstrap-20161124204912",
            "Neos.Party-20161124225257"
        ]
    }
}